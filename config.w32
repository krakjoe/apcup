ARG_ENABLE("apcup", "enable apcup support", "no");

if (PHP_APCUP != "no") {
	if (PHP_APCU == "yes" && ADD_EXTENSION_DEP('apcup', 'apcu')) {
		EXTENSION("apcup", "apcup.c");
	} else {
		WARNING("apcup support can't be enabled, apcu is not enabled")
		PHP_APCUP = "no"
	}
}

